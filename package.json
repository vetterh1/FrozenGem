    "react": "^16.13.1",
    "react-app-polyfill": "^1.0.6",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "^16.13.1",
    "react-gtm-module": "^2.0.11",
    "react-intl": "^4.3.1",
    "react-joyride": "^2.2.1",
