{
  "name": "frozen-gem",
  "version": "0.0.11",
  "private": true,
  "homepage": ".",
  "homepage-guithub": "https://vetterh1.github.io/frozengem/",
  "dependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@material-ui/core": "^4.7.2",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/pickers": "^3.2.8",
    "@material-ui/styles": "^4.7.1",
    "axios": "^0.19.0",
    "clsx": "^1.0.4",
    "date-fns": "^2.8.1",
    "exif-js": "^2.3.0",
    "intl": "^1.2.5",
    "notistack": "^0.9.7",
    "qs": "^6.9.1",
    "react": "^16.12.0",
    "react-app-polyfill": "^1.0.5",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "^16.12.0",
    "react-intl": "^3.9.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.0",
    "react-step-wizard": "github:vetterh1/react-step-wizard",
    "reselect": "^4.0.0",
    "source-map-explorer": "^2.1.2"
  },
  "scripts": {
    "start": "cross-env REACT_APP_VERSION=$npm_package_version react-scripts start",
    "build": "cross-env REACT_APP_VERSION=$npm_package_version react-scripts build",
    "startprod": "cross-env REACT_APP_VERSION=$npm_package_version /usr/local/bin/http-server ./build -p 8060 --cors",
    "startprod:old": "serve -s build",
    "test": "cross-env CI=true react-scripts test",
    "test:coverage": "npm test -- --coverage",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "i18n:extract": "MESSAGE_DIR='./.messages' react-scripts build",
    "i18n:manageTranslations": "node ./translationRunner.js",
    "postbuild": "find ./build -type f -name '*.js' -exec gzip -k '{}' \\;",
    "eject-be-careful-cannot-be-reverted": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "babel-plugin-macros": "^2.8.0",
    "compression-webpack-plugin": "^3.0.1",
    "cross-env": "^6.0.3",
    "react-intl-translations-manager": "^5.0.3",
    "react-redux": "^7.1.3",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0"
  }
}
