{
  "name": "frozen-gem",
  "version": "0.1.0",
  "private": true,
  "homepage-test": ".",
  "homepage": "https://vetterh1.github.io/frozengem/",
  "dependencies": {
    "@material-ui/core": "^4.0.0-beta.0",
    "@material-ui/icons": "^3.0.2",
    "@material-ui/styles": "^4.0.0-beta.0",
    "auth0-js": "^9.10.2",
    "axios": "^0.18.0",
    "clsx": "^1.0.4",
    "formik": "^1.5.4",
    "pusher-js": "^4.4.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-intl": "^2.8.0",
    "react-intl.macro": "^0.3.6",
    "react-router-dom": "^5.0.0",
    "react-scripts": "2.1.8",
    "react-step-wizard": "github:vetterh1/react-step-wizard",
    "source-map-explorer": "^1.8.0"
  },
  "scripts": {
    "start": "REACT_APP_BO_URL='http://localhost:9000' react-scripts start",
    "build": "react-scripts build",
    "startprod": "REACT_APP_BO_URL='https://food-maniac.com/frozenbo' serve -s build",
