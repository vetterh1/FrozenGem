{
  "name": "frozen-gem",
  "version": "0.0.4",
  "private": true,
  "homepage": ".",
  "homepage-guithub": "https://vetterh1.github.io/frozengem/",
  "dependencies": {
    "@date-io/date-fns": "^1.3.7",
    "@material-ui/core": "^4.1.1",
    "@material-ui/icons": "^4.2.0",
    "@material-ui/pickers": "^3.1.2",
    "@material-ui/styles": "^4.1.1",
    "axios": "^0.18.1",
    "clsx": "^1.0.4",
    "date-fns": "^2.0.0-beta.2",
    "exif-js": "^2.3.0",
    "intl": "^1.2.5",
    "notistack": "^0.8.6",
    "qs": "^6.7.0",
    "react": "^16.8.6",
    "react-app-polyfill": "^1.0.1",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.8.6",
    "react-intl": "^2.8.0",
    "react-intl.macro": "^0.3.6",
    "react-router-dom": "^5.0.1",
    "react-scripts": "2.1.8",
    "react-step-wizard": "github:vetterh1/react-step-wizard",
    "source-map-explorer": "^2.0.1"
  },
  "scripts": {
    "start": "REACT_APP_VERSION=$npm_package_version react-scripts start",
    "build": "REACT_APP_VERSION=$npm_package_version react-scripts build",
    "startprod": "REACT_APP_VERSION=$npm_package_version /usr/local/bin/http-server ./build -p 8060 --cors",
    "startprod:old": "serve -s build",
    "test": "CI=true react-scripts test",
    "test:coverage": "npm test -- --coverage",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "i18n:extract": "MESSAGE_DIR='./.messages' react-scripts build",
    "i18n:manageTranslations": "node ./translationRunner.js",
    "eject-be-careful-cannot-be-reverted": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "babel-plugin-macros": "^2.6.1",
    "compression-webpack-plugin": "^3.0.0",
    "react-intl-translations-manager": "^5.0.3"
  }
}
